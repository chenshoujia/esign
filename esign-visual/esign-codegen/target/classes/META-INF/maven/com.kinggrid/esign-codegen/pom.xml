<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>com.kinggrid</groupId>
		<artifactId>esign-visual</artifactId>
		<version>1.0.0</version>
	</parent>
	<artifactId>esign-codegen</artifactId>
	<description>代码生成模块</description>

	<dependencies>
		<!--接口文档-->
		<dependency>
			<groupId>com.kinggrid</groupId>
			<artifactId>esign-common-swagger</artifactId>
			<version>1.0.0</version>
		</dependency>
		<!--注册中心客户端-->
		<dependency>
			<groupId>com.alibaba.cloud</groupId>
			<artifactId>spring-cloud-starter-alibaba-nacos-discovery</artifactId>
		</dependency>
		<!--配置中心客户端-->
		<dependency>
			<groupId>com.alibaba.cloud</groupId>
			<artifactId>spring-cloud-starter-alibaba-nacos-config</artifactId>
		</dependency>
		<!--断路器依赖-->
		<dependency>
			<groupId>com.kinggrid</groupId>
			<artifactId>esign-common-sentinel</artifactId>
			<version>1.0.0</version>
		</dependency>
		<!--数据源-->
		<dependency>
			<groupId>com.kinggrid</groupId>
			<artifactId>esign-common-datasource</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>com.kinggrid</groupId>
			<artifactId>esign-common-mybatis</artifactId>
			<version>1.0.0</version>
		</dependency>
		<!--common-->
		<dependency>
			<groupId>com.kinggrid</groupId>
			<artifactId>esign-common-core</artifactId>
			<version>1.0.0</version>
		</dependency>
		<!--日志处理-->
		<dependency>
			<groupId>com.kinggrid</groupId>
			<artifactId>esign-common-log</artifactId>
			<version>1.0.0</version>
		</dependency>
		<!--安全模块-->
		<dependency>
			<groupId>com.kinggrid</groupId>
			<artifactId>esign-common-security</artifactId>
			<version>1.0.0</version>
		</dependency>
		<!--代码生成模板引擎-->
		<dependency>
			<artifactId>velocity</artifactId>
			<groupId>org.apache.velocity</groupId>
			<version>${velocity.version}</version>
		</dependency>
		<!--undertow容器-->
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-undertow</artifactId>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>
</project>